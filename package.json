{
  "name": "tsapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "clean-install": "rm -rf node_modules && yarn",
    "storybook": "storybook start -p 7007 -c ./src/Storybook",
    "eslint": "eslint --ext .jsx,.js ./"
  },
  "dependencies": {
    "@martin_hotell/rex-tils": "2.1.1",
    "apisauce": "1.0.2",
    "appcenter": "1.13.0",
    "appcenter-analytics": "1.13.0",
    "appcenter-crashes": "1.13.0",
    "currency.js": "1.2.1",
    "formik": "1.5.1",
    "lodash": "4.17.11",
    "native-base": "2.12.1",
    "react": "16.8.6",
    "react-native": "0.59.2",
    "react-native-gesture-handler": "1.1.0",
    "react-navigation": "3.5.1",
    "react-redux": "6.0.1",
    "realm": "2.25.0",
    "redux": "4.0.1",
    "redux-convenience-reducers": "github:Arcanfel/redux-convenience-reducers",
    "redux-form": "8.1.0",
    "redux-thunk": "2.3.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-transform-flow-strip-types": "7.4.0",
    "@babel/runtime": "7.4.2",
    "@storybook/addon-actions": "5.0.5",
    "@storybook/addon-links": "5.0.5",
    "@storybook/addons": "5.0.5",
    "@storybook/react": "5.0.5",
    "@storybook/react-native": "4.1.16",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-native": "0.57.42",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "3.0.6",
    "@types/react-redux": "7.0.5",
    "@types/react-test-renderer": "16.8.1",
    "@types/redux-form": "^8.1.1",
    "@types/redux-mock-store": "1.0.0",
    "@types/storybook__react": "4.0.1",
    "@types/storybook__react-native": "3.0.4",
    "@types/uuid": "3.4.4",
    "babel-jest": "24.5.0",
    "eslint": "5.15.3",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "jest": "24.5.0",
    "metro-react-native-babel-preset": "0.53.1",
    "prettier": "1.16.4",
    "prop-types": "15.7.2",
    "react-dom": "16.8.6",
    "react-native-typescript-transformer": "1.2.12",
    "react-test-renderer": "16.8.6",
    "reactotron-apisauce": "3.0.0",
    "reactotron-react-native": "3.2.1",
    "reactotron-redux": "3.1.0",
    "redux-mock-store": "1.5.3",
    "ts-jest": "24.0.0",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.3.4000"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/(?!react-native|realm)",
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js"
    ]
  }
}
